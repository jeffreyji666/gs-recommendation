<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl"  href="configuration.xsl"?>
<configuration>
	<property>
		<name>fs.defaultFS</name>
		<value>hdfs://ns</value>
	</property>
	<property>
		<name>dfs.nameservices</name>
		<value>ns</value>
	</property>
	<property>
		<name>dfs.replication</name>
		<value>2</value>
	</property>
	<property>
		<name>dfs.ha.namenodes.ns</name>
		<value>SVR2368HP360,SVR2369HP360</value>
	</property>
	<!--dfs.namenode.rpc-address.[nameservice ID]. -->
	<property>
		<name>dfs.namenode.rpc-address.ns.SVR2368HP360</name>
		<value>SVR2368HP360.hadoop.test.sh.ctriptravel.com:54310</value>
		<!--value>SVR2368HP360:54310</value -->
	</property>
	<property>
		<name>dfs.namenode.rpc-address.ns.SVR2369HP360</name>
		<value>SVR2369HP360.hadoop.test.sh.ctriptravel.com:54310</value>
		<!--value>SVR2369HP360:54310</value -->
	</property>
	<property>
		<name>dfs.namenode.servicerpc-address.ns.SVR2368HP360</name>
		<value>SVR2368HP360.hadoop.test.sh.ctriptravel.com:53310</value>
		<!--value>SVR2368HP360:53310</value -->
	</property>
	<property>
		<name>dfs.namenode.servicerpc-address.ns.SVR2369HP360</name>
		<value>SVR2369HP360.hadoop.test.sh.ctriptravel.com:53310</value>
		<!--value>SVR2369HP360:53310</value -->
	</property>
	<!--dfs.namenode.http-address.[nameservice ID]. -->
	<property>
		<name>dfs.namenode.http-address.ns.SVR2368HP360</name>
		<value>0.0.0.0:50070</value>
	</property>
	<property>
		<name>dfs.namenode.http-address.ns.SVR2369HP360</name>
		<value>0.0.0.0:50070</value>
	</property>
	<!--failover -->
	<property>
		<name>dfs.ha.automatic-failover.enabled</name>
		<value>true</value>
	</property>
	<property>
		<name>dfs.client.failover.proxy.provider.ns</name>
		<value>org.apache.hadoop.hdfs.server.namenode.ha.ConfiguredFailoverProxyProvider
		</value>
	</property>
	<property>
		<name>fs.hdfs.impl.disable.cache</name>
		<value>true</value>
	</property>
	<property>
        <name>output.compress.codec</name>
        <value>org.apache.hadoop.io.compress.SnappyCodec</value>
    </property>
</configuration>